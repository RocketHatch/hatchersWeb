<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <meta http-equiv="content-type" content="text/html; charset=iso-8859-1" /> 
    <meta name="title" content="Rocket City Starters" /> 
    <meta name="author" content="Larry Mason" /> 
    <meta name="description" content="Directory of creative professionals in Huntsville, Alabama Metro Area." />
    <!--meta name="image" content="http:alltowntech.com/images/hsvcity.jpg" /-->    
    <meta name="keywords" content="starters, creatives, entrepreneurial, creative professional, innovation hubs, infrastructure technology, startup ecosystem, startups" /> 
    <meta name="robots" content="all" /> 
    <link rel="icon" href="rh64.png" type="image/x-icon" />
    <link rel="shortcut icon" href="rh64.png" type="image/x-icon" />
    <link href="./stylesheets/rh.css" rel="stylesheet" type="text/css"/>
	<link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Raleway"/>
	<link href='http://fonts.googleapis.com/css?family=Open+Sans&subset=greek-ext,latin' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
	
    <title>Rocket Hatchers</title> 

    <script src="./javascripts/jquery-1.7.2.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCZ9EV1xuaqqq5Gn38WfDoQGsCzG4ARB6Y&sensor=false&libraries=visualization">
    </script>
	<script src="./javascripts/mhcCommon.js"></script>
	<script src="./javascripts/rcsDMods.js"></script> 
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.15/angular.min.js" type="text/javascript"></script>
    <script src="./javascripts/app.js" type="text/javascript"></script>
    <script src="./javascripts/rcsController.js" type="text/javascript"></script>	
    <script src="./javascripts/jquery.sha256.js"></script> 
    <script src="./javascripts/hprogCommon.js"></script>
    <script src="./javascripts/mhcSite.js"></script>
    <script src="./javascripts/mhcMap.js"></script>
	<script src="./javascripts/mhcAjax.js"></script>
	<script src="./javascripts/mhcLoad.js"></script>
	
    <script type="text/javascript">
      google.maps.event.addDomListener(window, 'load', initializeMap);
    </script>
  </head>
  <body id="bodycontainer" data-ng-app='mhcApp' data-ng-controller='RCSController'>    
    <div id="container">
    
      <div id="rhheader">
        <div id="rhmenubar">
		  <div id="bhm" class="rhmenubutn rhlink">Home</div>
		  <div id="bvl" class="rhmenubutn rhlink">Values</div>
		  <div id="bcm" class="rhmenubutn">Community</div>
		  <div id="bin" class="rhmenubutn rhlink">Initiatives</div>
		  <div id="bct" class="rhmenubutn rhlink">Contact</div>
		</div>		
      </div>
	  
	  <div id="rhmhead">      
        <div id='rhmenuback' class='closed'>
		  <div id="bhmm" class="rhmenubutnM rhlink"><div class='mmtxt'>Home</div></div>
		  <div id="bvlm" class="rhmenubutnM rhlink"><div class='mmtxt'>Values</div></div>
		  <div id="bcmm" class="rhmenubutnM active"><div class='mmtxt'>Community</div></div>
		  <div id="binm" class="rhmenubutnM rhlink"><div class='mmtxt'>Initiatives</div></div>
		  <div id="bctm" class="rhmenubutnM rhlink"><div class='mmtxt'>Contact</div></div>
		</div>
	    <div id='touchMenuBar'>
		  <div id='tigb'></div>
		  <div id='rtotopb'></div>
		</div>		
      </div>
	  
	  <!-- Main Page Content -->
	  <div id='mPage' class="page open">
        <a name="top"></a>
        <header id="pageHead">
	      <div id='pgTitle'>
	        <div id="logoName">Rocket Hatchers</div>
		  </div>
		  <div id='pgLogReg'>
	        <div id="logregtab">Welcome In</div>
		  </div>
	
		<!--div id="search">
		</div-->
				
		<!--div id="logReg">
		</div-->
		
        </header>
            
	    <div id="skillbar" class="closed">
		  <div id="sklhead">
		    <div id='btndwn'></div>
		    <div id="sklSrchTxt">find people with skills</div>
		   <!--div id="sklclr" class="select">clear all</div-->	  
		  </div>
		  <div id='skills'>
		   <!--div id='skslct' class='select'>select all</div-->
		    <div id='sk{{skill._id}}' class='skilltag' ng-repeat="skill in skillList" ng-click='updateFaces(skill._id)'>
			  <div class="sktxt">{{skill.type.toLowerCase()}}</div>
			</div>		  
		  </div>
	    </div>
	  
	    <div id="content">
		   <div id="memdiplay" class="column-center">	      
		     <div class="photoTile cover" data-ng-repeat="mem in members">
			   <img class="memigm" src="./images/members/{{mem._id}}.jpg"/>
			   <div class="idplate" data="{{mem.url}}" member="{{mem}}" ng-click='setCurrentMember(mem)'>
			     <div class="platename ptxt" data="{{mem}}" member="{{mem}}">{{mem.name.fname}} {{mem.name.lname}}</div>
				   <div class="plateskill ptxt">{{getSkills(mem.skillList)}}</div>
				 </div>
			   </div>
		   </div>				  
	    </div>
	  </div>
      <!-- end main page content -->
	  
      <!-- Profile page content --> 
      <div id='pPage' class="page">
	    <div id='proTop'> <!--data-ng-repeat="mbr in currentMember"-->
			<div id='proPct'>
			  <img class="pimg" src="./images/members/{{currentMember._id}}.jpg"/>
			</div>
			
			<div id='proBio'>
			  <div id='mname'>{{currentMember.name.fname.toLowerCase()}} {{currentMember.name.lname.toLowerCase()}}</div>
			  <div id='tag' class='protxt'>"{{currentMember.tagline}}"</div>
			  <div id='psbar'>
				<div id='psk{{$index + 1}}' class='proskilltag' data-ng-repeat="skl in curMemSkills">
				  <div class='skilltag active profile'>
					<div class="sktxt">{{skl}}</div>
				  </div>		  
				</div>
			  </div>
			  <!--div id='txtSendMess' class='mmessage'>
				<div id='mtrig' class='protxt' data='{{currentMember.uid}}'>send me a message</div>
			  </div-->
			  <div id='smbar'>
				<div id='smd{{$index+1}}' class="smLogos" data-ng-repeat="(typ,lnk) in currentMember.social" data='{{lnk}}'>
				  <img class="memigm" src="./images/mediaLogos/{{typ}}Fg.png" />
				<!--div class='smtype'>{{typ}}</div>
				<div class='smlnk'>{{lnk}}</div-->
				</div>
			  </div>
			  <div id='wlink' class='mylnk' ng-hide="! currentMember.url">
				<div id='mlnlbl'>my site: </div>
				<div id='mlntxt' data='{{currentMember.url}}'>{{currentMember.url}}</div>
			  </div>
			</div>
		</div>
        <div id='proBot'>		
			<div id='txtABio'>
			  <div id='ptxthead'></div>
			  <div id='ptxtarea'></div>
			</div>
			<div id='txtSendMess'>
			  <div id='mtxthead'></div>
			  <div id='mtxtarea'></div>
			</div>		
		</div>
	  </div>
	  <!-- end pofile page content -->
	  
      <footer id="pageFooter">
	    <div id="copyright" class="ftxt"> &copy; Copyright 2014 All Rights Reserved</div>
        <div id="fs" class="fsep separator ftxt">|</div>
   	    <div id="cre" class="ftxt">Created by</div>
        <div id="rhLink" class="ftxt rhlink">Rocket Hatch</div>		  
      </footer>
    </div> 
      <!-- Popups -->
    <div id="logregArea" class="puBlk log">
      <div id="logBx">
        <input id="untxt" type="text" class='log' placeholder="email"></input>
        <input id="pwtxt" type="text" class='log' placeholder="password"></input>
      </div>
	  <div id="regBx">
        <input id="profn" type="text" class='reg' placeholder="first name"></input>
        <input id="proln" type="text" class='reg' placeholder="last name"></input>
        <input id="proeml" type="text" class='reg' placeholder="email"></input>
        <input id="propw" type="text" class='reg' placeholder="password"></input>
        <input id="cpropw" type="text" class='reg' placeholder="confirm"></input>
        <input id="propfn" type="text" class='reg' placeholder="photo"></input>
        <input id="protag" type="text" class='reg' placeholder="tagline"></input>
        <input id="proweb" type="text" class='reg' placeholder="personal web site"></input>
      </div>
    </div>
      <!--div id="butArea">
        <div id='logRegButton' class="phoButton login"><div class="phoButtonText">Login</div></div>
      </div-->
    </div>
  
    <div id='messageArea' class='puBlk'>
	  <div id='messhead' >send message</div>
      <div id='messageBx' >
        <div id='message'>	    
          <div><input type="text" id="mfname" class='minput' placeholder="Your First"></input></div>
          <div><input type="text" id="mlname" class='minput' placeholder="Last Name"></input></div>
	      <div><input type="text" id="mphone" class='minput' placeholder="Phone"></input></div>
		  <div><input type="text" id="memail" class='minput' placeholder="Your email"></input></div>
		  <div><textarea id="sendtxt" class='minput message'></textarea></div>        
        </div>
      </div>
	  <div id='messfoot'>
        <div id='separator'></div>
        <p id="messcan" class="mDone"><a> cancel </a></p>
		<p id="messsep" class="mDone"><a> | </a></p>
		<p id="messsend" class="mDone"><a> send </a></p>
	  </div>
    </div>	
	
    <div id="glass"></div>
  </body>
</html>